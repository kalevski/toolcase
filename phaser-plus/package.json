{
    "name": "@toolcase/phaser-plus",
    "version": "0.7.12",
    "description": "üê† Makes PhaserJS framework more awesome",
    "source": "src/main.js",
    "main": "lib/main.js",
    "module": "lib/module.js",
    "types": "lib/main.d.ts",
    "scripts": {
        "dev": "nodemon -e js -w src --exec \"npm run build\"",
        "build": "npm run build:src && npm run build:tsd",
        "build:src": "parcel build src/main.js --no-source-maps --no-optimize",
        "build:tsd": "tsc -d --allowJs --types @toolcase/base,@toolcase/logging,phaser --emitDeclarationOnly --target es5 --lib ES2015,DOM,ScriptHost --outDir ./lib src/*.js"
    },
    "peerDependencies": {
        "@toolcase/base": "0.x",
        "@toolcase/logging": "0.x",
        "phaser": "3.x",
        "react": "18.x",
        "react-dom": "18.x"
    },
    "targets": {
        "types": false
    },
    "homepage": "https://kalevski.dev/toolcase",
    "keywords": [
        "phaser",
        "phaser3",
        "utilities",
        "games",
        "html5",
        "gamedev",
        "browser"
    ],
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kalevski/toolcase.git"
    },
    "bugs": {
        "url": "https://github.com/kalevski/toolcase/issues"
    },
    "license": "MIT"
}
