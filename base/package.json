{
    "name": "@toolcase/base",
    "version": "1.0.9",
    "description": "ðŸ§¬ Collection of JavaScript helper functions and structures",
    "source": "src/main.js",
    "main": "lib/main.js",
    "module": "lib/module.js",
    "types": "lib/main.d.ts",
    "scripts": {
        "dev": "nodemon -e js -w src --exec \"npm run build\"",
        "build": "npm run build:src && npm run build:tsd",
        "build:src": "parcel build --no-source-maps --no-cache --no-optimize",
        "build:tsd": "tsc -d --allowJs --emitDeclarationOnly --target es5 --lib ES2015 --outDir ./lib src/*.js"
    },
    "dependencies": {
        "eventemitter3": "^5.0.0",
        "protobufjs": "^7.2.0"
    },
    "targets": {
        "types": false,
        "main": {
            "optimize": false
        },
        "module": {
            "optimize": false
        }
    },
    "homepage": "https://kalevski.dev/toolcase",
    "keywords": [
        "helpers",
        "utils",
        "functions",
        "toolcase",
        "nodejs",
        "browser"
    ],
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kalevski/toolcase.git"
    },
    "bugs": {
        "url": "https://github.com/kalevski/toolcase/issues"
    },
    "license": "MIT"
}
